

<div class="row" style="text-align:right;padding:5px;">
    <a href="~/Empledos/Nuevo" class="btn btn-primary">Nuevo</a>
</div>
<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Edad</th>
                <th>Telefono</th>
                <th>Direccion</th>

                <th>Empresa</th>
                <th>Departamento</th>
                <th></th>
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.Id</td>
                    <td>@oElemento.nombre</td>
                    <td>@oElemento.edad</td>
                    <td>@oElemento.telefono</td>
                    <td>@oElemento.direccion</td>
                    <td>@oElemento.empresa</td>
                    <td>@oElemento.departamento</td>
                    <td>
                        <a class="btn btn-default" href="~/Empledos/Editar/@oElemento.Id">Editar</a>
                        <a class="btn btn-danger" href="~/Empledos/Eliminar/@oElemento.Id">Eliminar</a>
                    </td>
                </tr>

            }
        </table>
        <div>
            <a class="btn btn-block btn-default" href="/Home/Index">Regresar</a>
        </div>
    </div>

</div>

